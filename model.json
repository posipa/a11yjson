[
    {
        "model": "accessibility",
        "label": "Dostępność architektoniczna",
        "desc": "W sekcji \"Dostępność architektoniczna\" podmiot publiczny deklaruje dostępność architektoniczną swojej siedziby. <br />Podaje przy tym adres lub adresy, jeżeli lokalizacji jest kilka, do każdej tworząc oddzielne opisy.",
        "children": [
            {
                "model": "parking",
                "type": "fieldset",
                "label": "Parking",
                "children": [
                    {
                        "type": "radio",
                        "label": "Miejsca parkingowe dla osób niepełnosprawnych",
                        "model": "forWheelchairUsers",
                        "options": {
                            "1": "Możesz skorzystać z miejsca parkingowego. Na parkingu wyznaczyliśmy [number] miejsc przeznaczonych dla osób niepełnosprawnych. Pamiętaj o zabraniu karty parkingowej i umieszczeniu jej za szybą.",
                            "2": "Nie mamy wyznaczonych miejsc parkingowych dla osób niepełnosprawnych.",
                            "3": "Nie mamy parkingu."
                        },
                        "binding": {
                            "number": {
                                "type": "number",
                                "label": "Liczba miejsc parkingowych przeznaczonych dla osób niepełnosprawnych",
                                "model": "count"
                            }
                        }
                    },
                    {
                        "type": "bool",
                        "label": "Miejsca parkingowe są pomalowane na niebiesko i prawidłowo oznakowane.",
                        "model": "hasDedicatedSignage"
                    },
                    {
                        "type": "bool",
                        "label": "Od miejsca parkingowego do wejścia do budynku odległość wynosi około [number] metrów.",
// to chyba do usunięcia                        "model": "distanceToEntrance",
                        "binding": {
                            "number": {
                                "type": "number",
                                "label": "Odległość parkingu od wejścia",
                                "model": "distanceToEntrance"
                            }
                        }
                    },
                    {
                        "type": "radio",
                        "label": "Odpłatność za parking",
                        "model": "paymentBySpace",
                        "options": {
                            "1": "Parking jest bezpłatny.",
                            "2": "Parking jest bezpłatny dla osób posiadających kartę parkingową.",
                            "3": "Parking jest płatny."
                        }
                    },
                    {
                        "type": "textarea",
                        "label": "Opis usytuowania parkingu",
                        "desc": "Miejsca parkingowe znajdują się na dziedzińcu. Można na niego wjechać przez bramę. Szlaban otwiera strażnik, po okazaniu przepustki.", // to dlatego miałem wątpliwości. Tu wygląda to na placeholder, a nie dodatkowe wyjaśnienia
                        "model": "location"
                    }
                ]
            },
            {
                "model": "animalPolicy",
                "type": "fieldset",
                "label": "Pies asystujący",
                "children": [
                    {
                        "type": "bool",
                        "label": "Możesz przyjść z psem asystującym lub psem przewodnikiem. Pamiętaj o certyfikacie psa asystującego i zaświadczeniu o szczepieniu.",
                        "model": "allowsGuideDogs"
                    },
                    {
                        "type": "bool",
                        "label": "Pies musi mieć kaganiec. Nie dotyczy psów przewodników.",
                        "model": "dogsNeedMuzzle",
						"desc": "Poinformuj,jeżeli pies musi mieć kaganiec. Ten obowiązek nie dotyczy psów asystujących, co wynika wprost z przepisów. Dlatego oznakowany pies, po okazaniu certyfikatu, nie musi być w kagańcu."
                    },
                    {
                        "type": "bool",
                        "label": "Zapewniamy wodę dla zwierzaka.",
                        "model": "suppliesWaterForPets"
                    }
                ]
            },
            {
                "model": "entrance",
                "type": "fieldset",
                "label": "Wejście do budynku",
                "children": [
                    {
                        "type": "bool",
                        "label": "Wejście jest na poziomie gruntu. Nie maprzeszkód, by wjechać na wózku.",
                        "model": "isLevel"
                    },
                    {
                        "type": "bool",
                        "label": "Do wejścia prowadzą schody.",
                        "model": "stairs"
                    },
                    {
                        "type": "bool",
                        "label": "Przed schodami znajduje się pas ostrzegawczy wykonany z wypukłych elementów.",
                        "model": "hasTactileSafetyStrips",
                        "logic": {
                            "stairs": ":ischecked"
                        }
                    },
                    {
                        "type": "bool",
                        "label": "Stopnie są oznaczone kontrastowo.",
                        "model": "hasHighContrastNosing",
                        "logic": {
                            "stairs": ":ischecked"
                        }
                    },
                    {
                        "type": "bool",
                        "label": "Przy schodach znajduje się poręcz.",
                        "model": "hasHandRail",
                        "logic": {
                            "stairs": ":ischecked"
                        }
                    },
                    {
                        "type": "bool",
