                        "model": "hasFixedRamp",
                        "binding": {
                            "number": {
                                "type": "number",
                                "label": "Wartość nachylenia",
                                "model": "slopeAngle"
                            }
                        }
                    },
                    {
                        "type": "bool",
                        "label": "Istnieje możliwość rozłożenia przenośnej rampy. Zawiadom nas o tym wcześniej.",
