// to jest część dotycząca drzwi                        "model": "przestrzenManewrowaPrzedWejsciem",
                        "binding": {
                            "number": {
                                "type": "number",
                                "label": "Wielkość przestrzeni manewrowej",
                                "model": "turningSpaceInFront"
                            }
                        }
                    },
                    {
                        "type": "bool",
                        "label": "Drzwi są obrotowe.",
                        "model": "isRevolving"
                    },
                    {
                        "type": "bool",
                        "label": "Drzwi otwierają się automatycznie.",
                        "model": "isAutomaticOrAlwaysOpen"
                    },
                    {
                        "type": "bool",
                        "label": "Drzwi są zawsze otwarte.",
                        "model": "isAutomaticOrAlwaysOpen"
                    },
                    {
                        "type": "bool",
                        "label": "Drzwi musisz otworzyć ręcznie.",
// trzeba tu ustawić "isAutomaticOrAlwaysOpen"  na "false"                      "model": "mustHandOpen"
                    },
                    {
                        "type": "bool",
                        "label": "Otwierają się na zewnątrz.",
                        "model": "doorOpensToOutside"
                    },
                    {
                        "type": "radio",
                        "label": "Siła potrzebna do otwarcia drzwi",
                        "model": "isEasyToOpen",
                        "options": {
                            "1": "Otworzenie drzwi nie wymaga dużej siły.",
                            "2": "Otworzenie drzwi wymaga dużej siły."
                        }
                    },
                    {
                        "type": "bool",
                        "label": "Drzwi mają wygodną i ergonomiczną klamkę.",
                        "model": "hasErgonomicDoorHandle"
                    },
                    {
                        "type": "bool",
                        "label": "Można je łatwo utrzymać w pozycji otwartej.",
                        "model": "isEasyToHoldOpen"
                    },
                    {
                        "type": "bool",
                        "label": "Szerokość otworu wejściowego wynosi [number] centymetrów.",
// to niepotrzebne                        "model": "widthOfOpenDoors",
                        "binding": {
                            "number": {
                                "type": "number",
                                "label": "Szerokość w cm",
                                "model": "width"
                            }
                        }
                    },
                    {
                        "type": "bool",
                        "label": "Drzwi są przeźroczyste i [boolen] kontrastowo oznaczone.",
// to też do usunięcia                        "model": "transparentDoors",
                        "binding": {
                            "boolean": {
                                "type": "select",
                                "label": "Czy drzwi są kontrastowo oznaczone",
                                "model": "hasClearMarkingOnGlassDoor",
                                "options": {
                                    "są": "są", // A może tu dać odpowiednio "true" i "false" jako wartości
                                    "nie są": "nie są"
                                }
                            }
                        }
                    },
                    {
                        "type": "bool",
                        "label": "Skorzystaj z dzwonka przy drzwiach.",
                        "model": "useDoorBell" // to już Twój wkład
                    },
                    {
                        "type": "bool",
                        "label": "Skorzystaj z domofonu przy drzwiach.",
                        "model": "useIntercom"
                    },
					// zakładka
                    {
                        "type": "textarea",
                        "label": "Jeżeli nie możesz skorzystać z głównego wejścia",
                        "model": "ifCantUseEntrance"
                    }
                ]
